package com.itcast.zjw.testdrools

import com.itcast.zjw.drools.domain.Person;

rule "person"
	lock-on-active false
	when 
		$person1:Person();
		$person2:Person(age==$person1.age);
	then
		System.out.println("yunxing1\t"+$person1.getAge()+"\t"+$person1.getName());
		System.out.println("yunxing2\t"+$person2.getAge()+"\t"+$person2.getName());
		retract($person1);
		//retract($person2);
end